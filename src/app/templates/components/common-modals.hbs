{{#if commonModals.openedDbView}}
  {{modals/db-view-modal
    open=commonModals.openedDbView
    close=(action "closeModal" "dbView")
    dbViewId=commonModals.modalParams.dbViewId
  }}
{{/if}}

{{#if commonModals.openedProviderRedirect}}
  {{modals/provider-redirect
    open=commonModals.openedProviderRedirect
    close=(action "closeModal" "providerRedirect")
    space=commonModals.modalParams.space
    providers=commonModals.modalParams.providers
    deferredProviderChoice=commonModals.modalParams.deferredProviderChoice
  }}
{{/if}}
