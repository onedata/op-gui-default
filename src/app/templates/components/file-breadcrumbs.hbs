{{#if isLoading}}
  <div class="spinner-container spinner-32">
    {{spin-spinner scale=0.2 lines=12 length=20 width=10}}
  </div>
{{else}}
  <div class="file-breadcrumbs-list">
    {{#each filteredBreadcrumbsItems.content as |bitem|}}
      {{file-breadcrumbs-item item=bitem changeDir="changeDir"}}
    {{/each}}
  </div>
{{/if}}
